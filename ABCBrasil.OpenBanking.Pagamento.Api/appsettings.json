{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Serilog": {
    "IsNotEncrypt": "true",
    "Using": [ "Serilog.Exceptions", "Serilog", "Serilog.Sinks.Console", "Serilog.Sinks.Seq", "Serilog.Sinks.RollingFile" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Information",
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": "",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithExceptionDetails" ],
    "Properties": {
      "Environment": "Production"
    }
  },

  "Redis": {
    "AllowAdmin": true,
    "Ssl": false,
    "ConnectTimeout": 6000,
    "ConnectRetry": 2,
    "Database": 1,
    "Password": "",
    "Hosts": [
      {
        "Host": "",
        "Port": ""
      }
    ]
  },

  "ABCBrasilApiSettings": {
    "apiName": "ABCBrasil.OpenBanking.Pagamento.Api",
    "cacheActivated": true,
    "cacheTtl": 5,
    "keyName": "",
    "keyValue": "",

    "CalendarioConfig": {
      "BaseAddress": "",
      "ApiKeyName": "",
      "ApiKey": ""
    },
    "CoreCalculoConfig": {
      "BaseAdress": "",
      "ApiKeyName": "",
      "ApiKey": ""
    },
    "CorePagamentoLoteConfig": {
      "BaseAdress": "",
      "ApiKeyName": "",
      "Apikey": ""
    },
    "CoreComprovanteConfig": {
      "BaseAddress": "",
      "ApiKeyName": "",
      "Apikey": ""
    },
    "TibcoInclusaoPagamentoConfig": {
      "BaseAddress": "",
      "ApiKeyName": "",
      "ApiKey": ""
    }
  },
  "LogEventos": {
    "ApiKey": "",
    "FilaEMS": "abcbrasil.dev.corp.core.logeventos.inicio", //dev,qa,prd
    "UsuarioFila": "s_user_logevento",
    "NomeHeader": "CorrelationId",
    "ApiCodigo": 6, //API DDA
    "ApiCodigoCanal": 4, //OPEN BANKING PJ
    "XmlNameSpace": "http://www.abcbrasil.com.br/schemas/ABCBrasil.IB.LogEventos/GlobalCommonResources/SchemaDefinitions/ReceiveQueueLogEventosSchema.xsd"
